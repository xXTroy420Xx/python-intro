class Ally:
    pass

class Enemy:
    def __init__(self,name,hp,dmg,speed):
        self.name = name
        self.hp = hp
        self.dmg = dmg
        self.speed = speed
        
    def __str__(self):
        return f"{self.name} has {self.hp}"
        
    def speak(self,sound):
        return (f"{self.name} says {sound}")
    
    def damage(self,dmg):
        self.hp -= dmg
    
bg1 = Enemy("guy", 150, 75, 15)
bg2 = Enemy("bob", 15, 1, 1)

bg1.speak("why?")
